@using MvcOnlineTicariOtomasyon.Models.Siniflar
@model List<Urun>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<!DOCTYPE html>
<html lang="zxx">
<head>
    <meta charset="UTF-8">
    <meta name="description" content="Boto Photo Studio HTML Template">
    <meta name="keywords" content="photo, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Stylesheets -->
    <link rel="stylesheet" href="~/boto/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/boto/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/boto/css/slicknav.min.css" />
    <link rel="stylesheet" href="~/boto/css/fresco.css" />

    <!-- Main Stylesheets -->
    <link rel="stylesheet" href="~/boto/css/style.css" />


    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>


    <!-- About Page -->
    <div class="gallery__page">
        <div class="gallery__warp">
            <div class="row">
                @foreach (var item in Model)
                {
                    <div class="col-lg-3 col-md-4 col-sm-6">
                        <a class="gallery__item fresco" href="@item.UrunGorsel" data-fresco-group="gallery">
                            <img src="@item.UrunGorsel" alt="">
                        </a>
                    </div>
                }
            </div>
        </div>
    </div>
    <!-- About Page end -->
    <!--====== Javascripts & Jquery ======-->
    <script src="~/boto/js/vendor/jquery-3.2.1.min.js"></script>
    <script src="~/boto/js/jquery.slicknav.min.js"></script>
    <script src="~/boto/js/slick.min.js"></script>
    <script src="~/boto/js/fresco.min.js"></script>
    <script src="~/boto/js/main.js"></script>

</body>
</html>


